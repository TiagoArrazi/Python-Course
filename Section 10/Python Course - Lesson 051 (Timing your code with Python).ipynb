    "import time\n",
    "\n",
    "def powers(limit):\n",
    "    return [x ** 2 for x in range(limit)]\n",
    "\n",
    "def measure_runtime(func): #higher order function\n",
    "    start = time.time()\n",
    "    func()\n",
    "    end = time.time()\n",
    "    \n",
    "    return end - start\n",
    "\n",
    "print(measure_runtime(lambda: powers(90000000)))"
   ]
  }
